      <bpmndi:BPMNShape bpmnElement="submitBusinessTrip" id="BPMNShape_submitBusinessTrip">
        <omgdc:Bounds height="80.0" width="160.0" x="200.0" y="178.0" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="managerApproval" id="BPMNShape_managerApproval">
        <omgdc:Bounds height="80.0" width="140.0" x="410.0" y="178.0" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="managerDecision" id="BPMNShape_managerDecision">
        <omgdc:Bounds height="40.0" width="40.0" x="590.0" y="198.0" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="financeApproval" id="BPMNShape_financeApproval">
        <omgdc:Bounds height="80.0" width="120.0" x="690.0" y="178.0" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="financeDecision" id="BPMNShape_financeDecision">
        <omgdc:Bounds height="40.0" width="40.0" x="850.0" y="198.0" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="hrFiling" id="BPMNShape_hrFiling">
        <omgdc:Bounds height="80.0" width="120.0" x="950.0" y="178.0" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="tripConfirmation" id="BPMNShape_tripConfirmation">
        <omgdc:Bounds height="80.0" width="140.0" x="1120.0" y="178.0" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="rejectNotification" id="BPMNShape_rejectNotification">
        <omgdc:Bounds height="80.0" width="120.0" x="1120.0" y="310.0" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="endEvent" id="BPMNShape_endEvent">
        <omgdc:Bounds height="36.0" width="36.0" x="1310.0" y="206.0" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge bpmnElement="flow1" id="BPMNEdge_flow1">
        <omgdi:waypoint x="136.0" y="218.0" />
        <omgdi:waypoint x="200.0" y="218.0" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow2" id="BPMNEdge_flow2">
        <omgdi:waypoint x="360.0" y="218.0" />
        <omgdi:waypoint x="410.0" y="218.0" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow3" id="BPMNEdge_flow3">
        <omgdi:waypoint x="550.0" y="218.0" />
        <omgdi:waypoint x="590.0" y="218.0" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow4" id="BPMNEdge_flow4">
        <omgdi:waypoint x="610.0" y="238.0" />
        <omgdi:waypoint x="610.0" y="350.0" />
        <omgdi:waypoint x="1120.0" y="350.0" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow5" id="BPMNEdge_flow5">
        <omgdi:waypoint x="610.0" y="218.0" />
        <omgdi:waypoint x="690.0" y="218.0" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow6" id="BPMNEdge_flow6">
        <omgdi:waypoint x="810.0" y="218.0" />
        <omgdi:waypoint x="850.0" y="218.0" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow7" id="BPMNEdge_flow7">
        <omgdi:waypoint x="870.0" y="238.0" />
        <omgdi:waypoint x="870.0" y="350.0" />
        <omgdi:waypoint x="1120.0" y="350.0" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow8" id="BPMNEdge_flow8">
        <omgdi:waypoint x="870.0" y="218.0" />
        <omgdi:waypoint x="950.0" y="218.0" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow9" id="BPMNEdge_flow9">
        <omgdi:waypoint x="1070.0" y="218.0" />
        <omgdi:waypoint x="1120.0" y="218.0" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow10" id="BPMNEdge_flow10">
        <omgdi:waypoint x="1260.0" y="218.0" />
