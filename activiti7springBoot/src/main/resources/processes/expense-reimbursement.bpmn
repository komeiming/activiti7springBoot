<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns:xsd="http://www.w3.org/2001/XMLSchema"
             xmlns:activiti="http://activiti.org/bpmn"
             xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
             xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC"
             xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI"
             typeLanguage="http://www.w3.org/2001/XMLSchema"
             expressionLanguage="http://www.w3.org/1999/XPath"
             targetNamespace="http://www.activiti.org/test">

  <process id="expense-reimbursement" name="报销申请流程" isExecutable="true">
    <!-- 启动事件 -->
    <startEvent id="startEvent" name="开始" />
    
    <!-- 提交报销申请任务 -->
    <userTask id="submitExpense" name="提交报销申请" activiti:assignee="${applicant}">
      <extensionElements>
        <activiti:formProperty id="expenseDate" name="报销日期" type="date" required="true" />
        <activiti:formProperty id="amount" name="报销金额" type="double" required="true" />
        <activiti:formProperty id="currency" name="货币类型" type="enum" enumValues="CNY,USD,EUR" required="true" />
        <activiti:formProperty id="expenseType" name="报销类型" type="enum" enumValues="差旅费,办公费,招待费,其他" required="true" />
        <activiti:formProperty id="description" name="报销描述" type="string" required="true" />
        <activiti:formProperty id="receiptUrl" name="发票URL" type="string" required="true" />
        <activiti:formProperty id="projectId" name="项目ID" type="string" />
      </extensionElements>
    </userTask>
    
    <!-- 部门经理审批任务 -->
    <userTask id="managerApproval" name="部门经理审批" activiti:assignee="${manager}">
      <extensionElements>
        <activiti:formProperty id="approved" name="是否同意" type="boolean" required="true" />
        <activiti:formProperty id="comment" name="审批意见" type="string" />
      </extensionElements>
    </userTask>
    
    <!-- 网关判断 -->
    <exclusiveGateway id="managerDecision" name="经理审批决策" />
    
    <!-- 金额判断网关 -->
    <exclusiveGateway id="amountDecision" name="金额判断" />
    
    <!-- 财务审批任务 -->
    <userTask id="financeApproval" name="财务审批" activiti:assignee="finance">
      <extensionElements>
        <activiti:formProperty id="financeApproved" name="财务是否同意" type="boolean" required="true" />
        <activiti:formProperty id="financeComment" name="财务审批意见" type="string" />
        <activiti:formProperty id="receiptChecked" name="发票已审核" type="boolean" required="true" />
      </extensionElements>
    </userTask>
    
    <!-- 财务总监审批任务 -->
    <userTask id="cfoApproval" name="财务总监审批" activiti:assignee="cfo">
      <extensionElements>
        <activiti:formProperty id="cfoApproved" name="财务总监是否同意" type="boolean" required="true" />
        <activiti:formProperty id="cfoComment" name="财务总监审批意见" type="string" />
      </extensionElements>
    </userTask>
    
    <!-- 最终审批网关 -->
    <exclusiveGateway id="finalDecision" name="最终决策" />
    
    <!-- 拒绝通知任务 -->
    <userTask id="rejectNotification" name="拒绝通知" activiti:assignee="${applicant}">
      <extensionElements>
        <activiti:formProperty id="notificationRead" name="通知已读" type="boolean" required="true" />
      </extensionElements>
    </userTask>
    
    <!-- 打款任务 -->
    <userTask id="paymentTask" name="财务打款" activiti:assignee="finance">
      <extensionElements>
        <activiti:formProperty id="paymentDate" name="打款日期" type="date" required="true" />
        <activiti:formProperty id="paymentMethod" name="打款方式" type="enum" enumValues="银行转账,支付宝,微信" required="true" />
        <activiti:formProperty id="transactionId" name="交易ID" type="string" required="true" />
        <activiti:formProperty id="paymentCompleted" name="打款完成" type="boolean" required="true" />
      </extensionElements>
    </userTask>
    
    <!-- 完成通知任务 -->
    <userTask id="completionNotification" name="完成通知" activiti:assignee="${applicant}">
      <extensionElements>
        <activiti:formProperty id="notificationRead" name="通知已读" type="boolean" required="true" />
      </extensionElements>
    </userTask>
    
    <!-- 结束事件 -->
    <endEvent id="endEvent" name="结束" />
    
    <!-- 顺序流连接 -->
    <sequenceFlow id="flow1" sourceRef="startEvent" targetRef="submitExpense" />
    <sequenceFlow id="flow2" sourceRef="submitExpense" targetRef="managerApproval" />
    <sequenceFlow id="flow3" sourceRef="managerApproval" targetRef="managerDecision" />
    
    <!-- 经理审批拒绝 -->
    <sequenceFlow id="flow4" sourceRef="managerDecision" targetRef="rejectNotification">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approved == false}]]></conditionExpression>
    </sequenceFlow>
    
    <!-- 经理审批通过，进入金额判断 -->
    <sequenceFlow id="flow5" sourceRef="managerDecision" targetRef="amountDecision">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approved == true}]]></conditionExpression>
    </sequenceFlow>
    
    <!-- 金额大于10000，需要财务总监审批 -->
    <sequenceFlow id="flow6" sourceRef="amountDecision" targetRef="cfoApproval">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${amount > 10000}]]></conditionExpression>
    </sequenceFlow>
    
    <!-- 金额小于等于10000，直接财务审批 -->
    <sequenceFlow id="flow7" sourceRef="amountDecision" targetRef="financeApproval">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${amount <= 10000}]]></conditionExpression>
    </sequenceFlow>
    
    <!-- 财务总监审批通过 -->
    <sequenceFlow id="flow8" sourceRef="cfoApproval" targetRef="financeApproval">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${cfoApproved == true}]]></conditionExpression>
    </sequenceFlow>
    
    <!-- 财务总监审批拒绝 -->
    <sequenceFlow id="flow9" sourceRef="cfoApproval" targetRef="rejectNotification">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${cfoApproved == false}]]></conditionExpression>
    </sequenceFlow>
    
    <!-- 财务审批完成，进入最终决策 -->
    <sequenceFlow id="flow10" sourceRef="financeApproval" targetRef="finalDecision" />
    
    <!-- 财务审批通过，进入打款流程 -->
    <sequenceFlow id="flow11" sourceRef="finalDecision" targetRef="paymentTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${financeApproved == true}]]></conditionExpression>
    </sequenceFlow>
    
    <!-- 财务审批拒绝 -->
    <sequenceFlow id="flow12" sourceRef="finalDecision" targetRef="rejectNotification">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${financeApproved == false}]]></conditionExpression>
    </sequenceFlow>
    
    <!-- 打款完成，通知申请人 -->
    <sequenceFlow id="flow13" sourceRef="paymentTask" targetRef="completionNotification" />
    
    <!-- 结束流程 -->
    <sequenceFlow id="flow14" sourceRef="completionNotification" targetRef="endEvent" />
    <sequenceFlow id="flow15" sourceRef="rejectNotification" targetRef="endEvent" />
  </process>
  
