      <bpmndi:BPMNShape bpmnElement="departmentManagerApproval" id="BPMNShape_departmentManagerApproval">
        <omgdc:Bounds height="80.0" width="140.0" x="580.0" y="178.0" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="departmentDecision" id="BPMNShape_departmentDecision">
        <omgdc:Bounds height="40.0" width="40.0" x="760.0" y="198.0" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="hrApproval" id="BPMNShape_hrApproval">
        <omgdc:Bounds height="80.0" width="120.0" x="860.0" y="178.0" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="hrDecision" id="BPMNShape_hrDecision">
        <omgdc:Bounds height="40.0" width="40.0" x="1020.0" y="198.0" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="approvalNotification" id="BPMNShape_approvalNotification">
        <omgdc:Bounds height="80.0" width="140.0" x="1120.0" y="120.0" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="rejectNotification" id="BPMNShape_rejectNotification">
        <omgdc:Bounds height="80.0" width="140.0" x="1120.0" y="260.0" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="endEvent" id="BPMNShape_endEvent">
        <omgdc:Bounds height="36.0" width="36.0" x="1310.0" y="198.0" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge bpmnElement="flow1" id="BPMNEdge_flow1">
        <omgdi:waypoint x="136.0" y="218.0" />
        <omgdi:waypoint x="200.0" y="218.0" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow2" id="BPMNEdge_flow2">
        <omgdi:waypoint x="350.0" y="218.0" />
        <omgdi:waypoint x="390.0" y="218.0" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow3" id="BPMNEdge_flow3">
        <omgdi:waypoint x="530.0" y="218.0" />
        <omgdi:waypoint x="580.0" y="218.0" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow4" id="BPMNEdge_flow4">
        <omgdi:waypoint x="720.0" y="218.0" />
        <omgdi:waypoint x="760.0" y="218.0" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow5" id="BPMNEdge_flow5">
        <omgdi:waypoint x="780.0" y="238.0" />
        <omgdi:waypoint x="780.0" y="300.0" />
        <omgdi:waypoint x="1120.0" y="300.0" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow6" id="BPMNEdge_flow6">
        <omgdi:waypoint x="780.0" y="218.0" />
        <omgdi:waypoint x="860.0" y="218.0" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow7" id="BPMNEdge_flow7">
        <omgdi:waypoint x="980.0" y="218.0" />
        <omgdi:waypoint x="1020.0" y="218.0" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow8" id="BPMNEdge_flow8">
        <omgdi:waypoint x="1040.0" y="238.0" />
        <omgdi:waypoint x="1040.0" y="300.0" />
        <omgdi:waypoint x="1120.0" y="300.0" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow9" id="BPMNEdge_flow9">
        <omgdi:waypoint x="1040.0" y="198.0" />
        <omgdi:waypoint x="1040.0" y="160.0" />
        <omgdi:waypoint x="1120.0" y="160.0" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow10" id="BPMNEdge_flow10">
        <omgdi:waypoint x="1260.0" y="160.0" />
        <omgdi:waypoint x="1310.0" y="216.0" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow11" id="BPMNEdge_flow11">
        <omgdi:waypoint x="1260.0" y="300.0" />
