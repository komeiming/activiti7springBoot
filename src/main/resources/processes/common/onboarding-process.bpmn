      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow8" id="BPMNEdge_flow8">
        <omgdi:waypoint x="800.0" y="218.0" />
        <omgdi:waypoint x="850.0" y="218.0" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow9" id="BPMNEdge_flow9">
        <omgdi:waypoint x="870.0" y="198.0" />
        <omgdi:waypoint x="870.0" y="218.0" />
        <omgdi:waypoint x="950.0" y="218.0" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow10" id="BPMNEdge_flow10">
        <omgdi:waypoint x="870.0" y="238.0" />
