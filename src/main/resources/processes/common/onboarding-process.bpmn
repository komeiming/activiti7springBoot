<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns:xsd="http://www.w3.org/2001/XMLSchema"
             xmlns:activiti="http://activiti.org/bpmn"
             xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
             xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC"
             xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI"
             typeLanguage="http://www.w3.org/2001/XMLSchema"
             expressionLanguage="http://www.w3.org/1999/XPath"
             targetNamespace="http://www.activiti.org/test">

  <process id="onboarding-process" name="入职流程" isExecutable="true">
    <!-- 启动事件 -->
    <startEvent id="startEvent" name="开始" />
    
    <!-- 部门经理审批 -->
    <userTask id="managerApproval" name="部门经理审批" activiti:assignee="${departmentManager}">
      <extensionElements>
        <activiti:formProperty id="approved" name="是否同意" type="boolean" required="true" />
        <activiti:formProperty id="comment" name="审批意见" type="string" />
      </extensionElements>
    </userTask>
    
    <!-- 部门审批决策 -->
    <exclusiveGateway id="deptDecision" name="部门审批决策" />
    
    <!-- HR审批 -->
    <userTask id="hrApproval" name="HR审批" activiti:assignee="hr">
      <extensionElements>
        <activiti:formProperty id="hrApproved" name="HR是否同意" type="boolean" required="true" />
        <activiti:formProperty id="hrComment" name="HR审批意见" type="string" />
      </extensionElements>
    </userTask>
    
    <!-- HR审批决策 -->
    <exclusiveGateway id="hrDecision" name="HR审批决策" />
    
    <!-- 总经理审批 -->
    <userTask id="generalManagerApproval" name="总经理审批" activiti:assignee="generalManager">
      <extensionElements>
        <activiti:formProperty id="gmApproved" name="总经理是否同意" type="boolean" required="true" />
        <activiti:formProperty id="gmComment" name="总经理审批意见" type="string" />
      </extensionElements>
    </userTask>
    
    <!-- 总经理审批决策 -->
    <exclusiveGateway id="gmDecision" name="总经理审批决策" />
    
    <!-- 入职准备 -->
    <userTask id="onboardingPreparation" name="入职准备" activiti:assignee="hr">
      <extensionElements>
        <activiti:formProperty id="preparationCompleted" name="准备已完成" type="boolean" required="true" />
        <activiti:formProperty id="preparationDetails" name="准备详情" type="string" />
      </extensionElements>
    </userTask>
    
    <!-- 入职执行 -->
    <userTask id="onboardingExecution" name="入职执行" activiti:assignee="${departmentManager}">
      <extensionElements>
        <activiti:formProperty id="executionCompleted" name="执行已完成" type="boolean" required="true" />
        <activiti:formProperty id="executionDetails" name="执行详情" type="string" />
      </extensionElements>
    </userTask>
    
    <!-- 入职完成通知 -->
    <userTask id="completionNotification" name="入职完成通知" activiti:assignee="${applicant}">
      <extensionElements>
        <activiti:formProperty id="notificationRead" name="通知已读" type="boolean" required="true" />
      </extensionElements>
    </userTask>
    
    <!-- 拒绝通知 -->
    <userTask id="rejectNotification" name="拒绝通知" activiti:assignee="${applicant}">
      <extensionElements>
        <activiti:formProperty id="notificationRead" name="通知已读" type="boolean" required="true" />
      </extensionElements>
    </userTask>
    
    <!-- 结束事件 -->
    <endEvent id="endEvent" name="结束" />
    
    <!-- 顺序流连接 -->
    <sequenceFlow id="flow1" sourceRef="startEvent" targetRef="managerApproval" />
    <sequenceFlow id="flow2" sourceRef="managerApproval" targetRef="deptDecision" />
    
    <!-- 部门审批通过 -->
    <sequenceFlow id="flow3" sourceRef="deptDecision" targetRef="hrApproval">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approved == true}]]></conditionExpression>
    </sequenceFlow>
    
    <!-- 部门审批拒绝 -->
    <sequenceFlow id="flow4" sourceRef="deptDecision" targetRef="rejectNotification">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approved == false}]]></conditionExpression>
    </sequenceFlow>
    
    <sequenceFlow id="flow5" sourceRef="hrApproval" targetRef="hrDecision" />
    
    <!-- HR审批通过 -->
    <sequenceFlow id="flow6" sourceRef="hrDecision" targetRef="generalManagerApproval">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${hrApproved == true}]]></conditionExpression>
    </sequenceFlow>
    
    <!-- HR审批拒绝 -->
    <sequenceFlow id="flow7" sourceRef="hrDecision" targetRef="rejectNotification">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${hrApproved == false}]]></conditionExpression>
    </sequenceFlow>
    
    <sequenceFlow id="flow8" sourceRef="generalManagerApproval" targetRef="gmDecision" />
    
    <!-- 总经理审批通过 -->
    <sequenceFlow id="flow9" sourceRef="gmDecision" targetRef="onboardingPreparation">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${gmApproved == true}]]></conditionExpression>
    </sequenceFlow>
    
    <!-- 总经理审批拒绝 -->
    <sequenceFlow id="flow10" sourceRef="gmDecision" targetRef="rejectNotification">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${gmApproved == false}]]></conditionExpression>
    </sequenceFlow>
    
    <sequenceFlow id="flow11" sourceRef="onboardingPreparation" targetRef="onboardingExecution" />
    <sequenceFlow id="flow12" sourceRef="onboardingExecution" targetRef="completionNotification" />
    <sequenceFlow id="flow13" sourceRef="completionNotification" targetRef="endEvent" />
    <sequenceFlow id="flow14" sourceRef="rejectNotification" targetRef="endEvent" />
  </process>
  
  <!-- 图形信息 -->
  <bpmndi:BPMNDiagram id="BPMNDiagram_onboarding-process">
    <bpmndi:BPMNPlane bpmnElement="onboarding-process" id="BPMNPlane_onboarding-process">
      <bpmndi:BPMNShape bpmnElement="startEvent" id="BPMNShape_startEvent">
        <omgdc:Bounds height="36.0" width="36.0" x="100.0" y="200.0" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="managerApproval" id="BPMNShape_managerApproval">
        <omgdc:Bounds height="80.0" width="100.0" x="200.0" y="178.0" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="deptDecision" id="BPMNShape_deptDecision">
        <omgdc:Bounds height="40.0" width="40.0" x="350.0" y="198.0" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="hrApproval" id="BPMNShape_hrApproval">
        <omgdc:Bounds height="80.0" width="100.0" x="450.0" y="178.0" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="hrDecision" id="BPMNShape_hrDecision">
        <omgdc:Bounds height="40.0" width="40.0" x="600.0" y="198.0" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="generalManagerApproval" id="BPMNShape_generalManagerApproval">
        <omgdc:Bounds height="80.0" width="100.0" x="700.0" y="178.0" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="gmDecision" id="BPMNShape_gmDecision">
        <omgdc:Bounds height="40.0" width="40.0" x="850.0" y="198.0" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="onboardingPreparation" id="BPMNShape_onboardingPreparation">
        <omgdc:Bounds height="80.0" width="100.0" x="950.0" y="178.0" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="onboardingExecution" id="BPMNShape_onboardingExecution">
        <omgdc:Bounds height="80.0" width="100.0" x="1100.0" y="178.0" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="completionNotification" id="BPMNShape_completionNotification">
        <omgdc:Bounds height="80.0" width="100.0" x="1250.0" y="178.0" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="rejectNotification" id="BPMNShape_rejectNotification">
        <omgdc:Bounds height="80.0" width="100.0" x="600.0" y="300.0" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="endEvent" id="BPMNShape_endEvent">
        <omgdc:Bounds height="36.0" width="36.0" x="1400.0" y="218.0" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge bpmnElement="flow1" id="BPMNEdge_flow1">
        <omgdi:waypoint x="136.0" y="218.0" />
        <omgdi:waypoint x="200.0" y="218.0" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow2" id="BPMNEdge_flow2">
        <omgdi:waypoint x="300.0" y="218.0" />
        <omgdi:waypoint x="350.0" y="218.0" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow3" id="BPMNEdge_flow3">
        <omgdi:waypoint x="370.0" y="198.0" />
        <omgdi:waypoint x="370.0" y="218.0" />
        <omgdi:waypoint x="450.0" y="218.0" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow4" id="BPMNEdge_flow4">
        <omgdi:waypoint x="370.0" y="238.0" />
        <omgdi:waypoint x="370.0" y="300.0" />
        <omgdi:waypoint x="600.0" y="300.0" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow5" id="BPMNEdge_flow5">
        <omgdi:waypoint x="550.0" y="218.0" />
        <omgdi:waypoint x="600.0" y="218.0" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow6" id="BPMNEdge_flow6">
        <omgdi:waypoint x="620.0" y="198.0" />
        <omgdi:waypoint x="620.0" y="218.0" />
        <omgdi:waypoint x="700.0" y="218.0" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow7" id="BPMNEdge_flow7">
        <omgdi:waypoint x="620.0" y="238.0" />
